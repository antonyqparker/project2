<h2>User Information</h2>
<% @user.errors.full_messages.each do |error| %>
  <div class="alert alert-danger alert-dismissible" role="alert">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <%= error %>
  </div>
<% end %>
<form id="information_upload" method="post" action="/users/<%= @user.id %>" enctype="multipart/form-data">
  <input type="hidden" name="_method" value="put"></input>
  <div class="form-group">
    <label for="age">Age</label>
    <input name="user[age]" type="text" class="form-control" id="user_age" placeholder="Age" value="<%= @user.age %>">
  </div>
  <div class="form-group">
    <label for="level">Level</label>
    <input name="user[level]" type="text" class="form-control" id="user_level" placeholder="Level" value="<%= @user.level %>">
  </div>
  <div class="form-group">
    <label for="gender">Gender</label>
    <input name="user[gender]" type="text" class="form-control" id="user_gender" placeholder="Gender" value="<%= @user.gender %>">
  </div>
  <button type="submit" class="btn btn-default">Update</button>
</form>